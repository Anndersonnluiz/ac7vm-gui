<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="sistemagsPU" transaction-type="RESOURCE_LOCAL">
		<class>br.com.sistemags.model.Usuario</class>
		<class>br.com.sistemags.model.Acesso</class>
		<class>br.com.sistemags.model.Grupoacesso</class>
		<class>br.com.sistemags.model.Celula</class>
		<class>br.com.sistemags.model.Equipe</class>
		<class>br.com.sistemags.model.Parceirodeusanual</class>
		<class>br.com.sistemags.model.Parceirodeusmensal</class>
		<class>br.com.sistemags.model.Subequipe</class>
		<class>br.com.sistemags.model.Usuarioequipe</class>
		<class>br.com.sistemags.model.Usuariosubequipe</class>
		<class>br.com.sistemags.model.Feednoticias</class>
		
		
		
	<properties>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
			<property name="javax.persistence.jdbc.user" value="root"/>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:8081/sistemags"/>
		</properties>
	</persistence-unit>
	
</persistence>
